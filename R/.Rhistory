library(dplyr)
library(tidyr)
library(ggplot2)
library(moments)
library(corrplot)
library(lavaan)
library(tidySEM)
# Report libraries
library(knitr)
library(kableExtra)
library(rmarkdown)
# Load data variables
df <- foreign::read.spss(
"data/data1.sav", use.value.labels = FALSE, to.data.frame = TRUE
)
# Z-tranform
df_z <- sapply(
df, function(x) (x - mean(x, na.rm = TRUE)) / sd(x, na.rm = TRUE)
) %>% as.data.frame()
# Check histogram
df_z_long <- df_z %>%
tidyr::pivot_longer(dplyr::everything(), names_to = "variable")
